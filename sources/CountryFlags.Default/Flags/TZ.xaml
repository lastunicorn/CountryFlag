<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Tanzania -->

    <Canvas x:Key="CountryFlag_TZ" Width="72" Height="48">

        <Path
            Data="M0,48L0,0 72,0z"
            Fill="#FF1EB53A" />

        <Path
            Data="M72,0L72,48 0,48z"
            Fill="#FF00A3DD" />

        <Path
            Data="M0,48L72,0"
            Stroke="#FFFCD116"
            StrokeThickness="19"
            Clip="m0 0h72v48H0z" />

        <Path
            Data="M0,48L72,0"
            Stroke="#FF000000"
            StrokeThickness="13"
            Clip="m0 0h72v48H0z" />

    </Canvas>

</ResourceDictionary>