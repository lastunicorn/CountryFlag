<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Andorra (Second flag - 1866-1939) -->

    <Canvas x:Key="CountryFlag_AD_Second" Width="1000" Height="700">

        <Path
            Data="F1M0,0L1000,0 1000,700 0,700z"
            Fill="#FFD0103A" />

        <Path
            Data="F1M0,0L680,0 680,700 0,700z"
            Fill="#FFFEDF00" />

        <Path
            Data="F1M0,0L320,0 320,700 0,700z"
            Fill="#FF0018A8" />

    </Canvas>

</ResourceDictionary>