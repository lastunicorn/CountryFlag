<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Andorra (First flag - 1806-1866) -->

    <Canvas x:Key="CountryFlag_AD_First" Width="6" Height="4">

        <Path
            Data="F1M0,0L6,0 6,4 0,4z"
            Fill="#FFD81C3F" />

        <Path
            Data="F1M0,0L3,0 3,4 0,4z"
            Fill="#FFFCE016" />

    </Canvas>

</ResourceDictionary>