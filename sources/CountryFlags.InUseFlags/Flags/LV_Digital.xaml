<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Latvia (Digital flag) -->

    <Canvas x:Key="CountryFlag_LV_Digital" Width="1200" Height="600">

        <Path
            Data="F1M0,0L1200,0 1200,600 0,600"
            Fill="#FF9D2235" />

        <Path
            Data="F1M0,240L1200,240 1200,360 0,360"
            Fill="#FFFFFFFF" />

    </Canvas>

</ResourceDictionary>