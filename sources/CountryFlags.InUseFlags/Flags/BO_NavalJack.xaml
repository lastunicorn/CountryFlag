<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Bolivia (Naval Jack) -->

    <Canvas x:Key="CountryFlag_BO_NavalJack" Width="6" Height="6">

        <Rectangle
            Fill="#FFCE1126"
            Width="6"
            Height="6" />
        
        <Rectangle
            Fill="#FFF9D616"
            Width="4"
            Height="4"
            Canvas.Left="1"
            Canvas.Top="1" />

        <Rectangle
            Fill="#FF007A3D"
            Width="2"
            Height="2"
            Canvas.Left="2"
            Canvas.Top="2" />

    </Canvas>

</ResourceDictionary>