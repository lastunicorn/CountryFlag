<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Russia (Naval Ensign) -->

    <Canvas x:Key="CountryFlag_RU_NavalEnsign" Width="30" Height="20">

        <Rectangle
            Fill="#FFFFFFFF"
            Width="30"
            Height="20" />

        <Path
            Data="M0,0L30,20 M0,20L30,0"
            Fill="#FF000000"
            Stroke="#FF0032A0"
            StrokeThickness="3"
            Clip="M 0 0 H30 V 20 H 0 z"/>

    </Canvas>

</ResourceDictionary>