<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Republic of Congo -->

    <Canvas x:Key="CountryFlag_CG" Width="3" Height="2">

        <Rectangle
            Width="3"
            Height="2"
            Fill="#fbde4a" />

        <Path
            Data="m 0 2 v -2 h 2 z"
            Fill="#009543" />

        <Path
            Data="m 3 0 v 2 h -2 z"
            Fill="#da1a35" />

    </Canvas>

</ResourceDictionary>