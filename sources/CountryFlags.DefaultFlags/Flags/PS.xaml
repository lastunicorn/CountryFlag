<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Palestine -->

    <Canvas x:Key="CountryFlag_PS" Width="6" Height="3">

        <Rectangle
            Width="6"
            Height="3"
            Fill="#FFFFFFFF" />

        <Rectangle
            Width="6"
            Height="1"
            Fill="#FF000000" />

        <Rectangle
            Width="6"
            Height="1"
            Canvas.Top="2"
            Fill="#009736" />

        <Polygon
            Points="0,0 2,1.5 0,3"
            Fill="#EE2A35" />

    </Canvas>

</ResourceDictionary>