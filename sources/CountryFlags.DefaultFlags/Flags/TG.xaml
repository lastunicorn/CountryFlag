<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Togo -->

    <Canvas x:Key="CountryFlag_TG" Width="809.016994375" Height="500">

        <Path
            Data="M0,0L809.016994375,0 809.016994375,500 0,500z"
            Fill="#FF006A4E" />

        <Path
            Data="M0,100L809.016994375,100 809.016994375,200 0,200z"
            Fill="#FFFFCE00" />

        <Path
            Data="M0,0L300,0 300,350 0,350z"
            Fill="#FFD21034" />

        <Path
            Data="M0,300L809.016994375,300 809.016994375,400 0,400z"
            Fill="#FFFFCE00" />

        <!-- Star -->

        <Path
            Data="F1 m 0 -1 0.5878 1.809 -1.5388 -1.118 h 1.902 l -1.5388 1.118 z"
            Fill="#FFFFFFFF">

            <Path.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="95" ScaleY="95" />
                    <TranslateTransform X="150" Y="150" />
                </TransformGroup>
            </Path.RenderTransform>
        </Path>

    </Canvas>

</ResourceDictionary>