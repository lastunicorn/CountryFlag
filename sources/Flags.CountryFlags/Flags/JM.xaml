<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Jamaica -->

    <Canvas x:Key="CountryFlag_JM" Width="12" Height="6">

        <Path
            Fill="#007749"
            Data="m0 0h12v6H0z"/>
        
        <Path
            Stroke="#ffb81c"
            Fill="#000000"
            Data="m -2 -1 v 8 l 16 -8 v 8 z"
            Clip="m 0 0 h 12 v 6 H 0 z"/>

    </Canvas>

</ResourceDictionary>